<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/material_list_three_line_normal_height">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/material_list_text_first_position_margin_start"
        android:layout_marginLeft="@dimen/material_list_text_first_position_margin_start"
        android:layout_marginEnd="@dimen/material_list_text_last_position_margin_end"
        android:layout_marginRight="@dimen/material_list_text_last_position_margin_end"
        android:layout_gravity="center_vertical"
        android:orientation="vertical">
        <TextView
            android:id="@+id/primary_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/material_list_text_gutter"
            android:textSize="@dimen/material_list_normal_primary_text_size"
            android:textColor="@color/material_typography_primary_text_color_dark"
            android:maxLines="1"
            android:ellipsize="end"
            tools:text="@string/primary_text_long"/>

        <TextView
            android:id="@+id/secondary_text_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/material_list_text_gutter"
            android:textSize="@dimen/material_list_normal_secondary_text_size"
            android:textColor="@color/material_typography_secondary_text_color_dark"
            android:lines="1"
            android:ellipsize="end"
            tools:text="@string/secondary_text_long"/>

        <TextView
            android:id="@+id/secondary_text_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/material_list_normal_secondary_text_size"
            android:textColor="@color/material_typography_secondary_text_color_dark"
            android:lines="1"
            android:ellipsize="end"
            tools:text="@string/secondary_text_long"/>
    </LinearLayout>

</FrameLayout>